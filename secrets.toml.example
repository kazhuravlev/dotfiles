# Example secrets configuration file for work computer
# Copy this file to ~/.config/chezmoi/secrets.toml and fill in your actual values

# GitHub Personal Access Token for Homebrew
HOMEBREW_GITHUB_API_TOKEN = "ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

# Go private repositories (comma-separated)
GOPRIVATE = "github.com/mycompany,gitlab.com/myteam"

# Ngrok authentication token
NGROK_AUTH_TOKEN = "2xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"

# Add any other secrets you need here
# EXAMPLE_API_KEY = "sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
